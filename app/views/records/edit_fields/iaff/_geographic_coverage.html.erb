<%= f.input key,
				as: :multi_value,
				placeholder: 'Search for a location',
				input_html: {
					class: 'form-control',
					data: { 'autocomplete-url' => "/authorities/search/local/iaff_geographic_coverage",
									'autocomplete' => key }
				},
				### Required for the ControlledVocabulary javascript:
				wrapper_html: { data: { 'autocomplete-url' => "/authorities/search/local/iaff_geographic_coverage",
																'field-name' => key }},
				required: f.object.required?(key) %>