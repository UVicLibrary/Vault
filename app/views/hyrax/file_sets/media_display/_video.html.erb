  <video controls="controls" id="video" controlsList="nodownload" class="video-js vjs-default-skin" data-setup="{}" preload="auto" style="max-width:100%;">
    <source src="<%= hyrax.download_path(file_set) %>" type="video/mp4" />
    <source src="<%= hyrax.download_path(file_set, file: 'webm') %>" type="video/webm" />
    Your browser does not support the video tag.
  </video><br/>
  <% if allow_downloads?(file_set) %>
    <%= link_to t('hyrax.file_set.show.downloadable_content.video_link'),
                hyrax.download_path(file_set),
                data: { label: file_set.id },
                target: :_blank,
                id: "file_download" %>
  <% end %>

  <%# Include link to transcript %>
  <% if file_set.parent.member_presenters.any? { |fs| fs.pdf? } %>
    <% transcript = file_set.parent.member_presenters.find { |fs| fs.pdf? } %>
    &ensp;|&ensp;
    <%= link_to "Download transcript (PDF)",
                hyrax.download_path(transcript),
                data: { turbolinks: false },
                target: :_blank,
                id: "transcript_file_download",
                data: { label: transcript.id } %>
  <% end %>
